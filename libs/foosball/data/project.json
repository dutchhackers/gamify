{
  "root": "libs/foosball/data",
  "sourceRoot": "libs/foosball/data/src",
  "projectType": "library",
  "targets": {
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/foosball/data/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/foosball/data"],
      "options": {
        "jestConfig": "libs/foosball/data/jest.config.js",
        "passWithNoTests": true
      },
      "dependsOn": [
        {
          "target": "generate",
          "projects": "self"
        }
      ]
    }
  },
  "tags": ["type:util", "scope:foosball"]
}
