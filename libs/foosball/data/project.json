{
  "root": "libs/foosball/data",
  "sourceRoot": "libs/foosball/data/src",
  "projectType": "library",
  "targets": {
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/foosball/data/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/foosball/data"],
      "options": {
        "jestConfig": "libs/foosball/data/jest.config.js",
        "passWithNoTests": true
      }
    },
    "generate": {
      "builder": "@nx-tools/nx-prisma:generate",
      "options": {
        "schema": "libs/foosball/data/prisma/schema.prisma"
      }
    },
    "push": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "prisma db push --schema libs/foosball/data/prisma/schema.prisma"
      },
      "parallel": false
    },
    "seed": {
      "builder": "@nx-tools/nx-prisma:seed",
      "options": {
        "script": "libs/foosball/data/prisma/seed.ts",
        "tsConfig": "libs/foosball/data/prisma/tsconfig.tools.json"
      }
    }
  },
  "tags": ["type:util", "scope:foosball"]
}
